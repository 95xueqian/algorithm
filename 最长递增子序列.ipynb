   "source": [
    "array = [3,2,4,6,5,7,8,9,10]\n",
    "\n",
    "def maxlength(array):\n",
    "    F = [1]*len(array)\n",
    "    res = 1\n",
    "    for i in range(1,len(array)):\n",
    "        if array[i] > array[i-1]:\n",
    "            F[i] = F[i-1]+1\n",
    "            res = max(F[i], res)\n",
    "    \n",
    "    return res\n",
    "\n",
    "length = maxlength(array)\n",
    "print(length)"
   ]
